{
  "name": "trimstring",
  "version": "0.2.3",
  "description": "Neatly trim template strings",
  "license": "MIT",
  "repository": "iansinnott/trimstring",
  "main": "dist/trimstring.js",
  "author": {
    "name": "Ian Sinnott",
    "email": "ian@iansinnott.com",
    "url": "iansinnott.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "ava",
    "build": "webpack",
    "bump": "npm version patch -m \"v%s\"",
    "bump:minor": "npm version minor -m \"v%s\"",
    "bump:major": "npm version major -m \"v%s\"",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ]
  },
  "files": [
    "src/index.js"
  ],
  "keywords": [
    "trim",
    "string",
    "template",
    "interpolation"
  ],
  "dependencies": {
    "debug": "^2.6.0",
    "ramda": "^0.23.0",
    "shirt": "^0.1.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "webpack": "^2.2.0"
  }
}
